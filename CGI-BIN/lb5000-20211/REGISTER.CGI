# getmember2 ·ÀÖ¹±äÁ¿»ìÂÒ by LitChi
sub getmember2 {
	my ($nametocheck, $filetoopen);
	
	$nametocheck = shift;
	$nametocheck =~ s/ /\_/g;
	$nametocheck =~ tr/A-Z/a-z/;
	undef $filedata;
	$filetoopen = "$lbdir" . "$memdir/$nametocheck.cgi";
	$filetoopen = &stripMETA($filetoopen);
	if (-e $filetoopen) {
            open(FILE9,"$filetoopen");
            my $filedata = <FILE9>;
            close(FILE9);
	    chomp($filedata);
	    ($lmembername, $lpassword, $lmembertitle, $lmembercode, $lnumberofposts, $lemailaddress, $lshowemail, $lipaddress, $lhomepage, $laolname, $licqnumber ,$llocation ,$linterests, $ljoineddate, $llastpostdate, $lsignature, $ltimedifference, $lprivateforums, $luseravatar, $luserflag, $luserxz, $lusersx, $lpersonalavatar, $lpersonalwidth, $lpersonalheight, $lrating, $llastgone, $lvisitno, $laddjy, $lmeili, $lmymoney, $lpostdel, $lsex, $leducation, $lmarry, $lwork, $lborn, $luseradd1, $luseradd2) = split(/\t/,$filedata);
	    ($lnumberofposts, $lnumberofreplys) = split(/\|/,$lnumberofposts);
            chomp $privateforums;
            if($privateforums) {
		my(@private) = split(/&/,$privateforums);
                my $accessallowed;
		foreach $accessallowed (@private) {
		    chomp $accessallowed;
		    my ($access, $value) = split(/=/,$accessallowed);
		    $allowedentry{$access} = $value;
		}
	    }
	}
}

# recommender's meili added by LitChi 2001/7/25
sub recommfunc {
   my $recommender = shift;
   &getmember2("$recommender");
   $addtjml = 0 if ($addtjml eq "");
   $addtjjy = 0 if ($addtjjy eq "");
   $addtjhb = 0 if ($addtjhb eq "");
   
   if ($lmeili eq "") { $lmeili = $addtjml; }
   else { $lmeili += $addtjml; }

   if ($lmymoney eq "") { $lmymoney = $addtjhb; }
   else { $lmymoney += $addtjhb; }

   if ($laddjy eq "") { $laddjy = $addtjjy; }
   else { $laddjy += $addtjjy; }
        
   my($recommfile, $filetomake);
   $recommfile = $recommender;
   $recommfile =~ y/ /_/;
   $recommfile =~ tr/A-Z/a-z/;
   $filetomake = "$lbdir" . "$memdir/$recommfile.cgi";
   my $filetoopens = &lockfilename($filetomake);
   if ((!(-e "$filetoopens.lck"))&&($lpassword ne "")) {
	&winlock($filetomake) if ($OS_USED eq "Nt");
	if (open(FILE7, ">$filetomake")) {
	flock(FILE7, 2) if ($OS_USED eq "Unix");
	print FILE7 "$lmembername\t$lpassword\t$lmembertitle\t$lmembercode\t$lnumberofposts|$lnumberofreplys\t$lemailaddress\t$lshowemail\t$lipaddress\t$lhomepage\t$laolname\t$licqnumber\t$llocation\t$linterests\t$ljoineddate\t$llastpostdate\t$lsignature\t$ltimedifference\t$lprivateforums\t$linuseravatar\t$luserflag\t$luserxz\t$lusersx\t$lpersonalavatar\t$lpersonalwidth\t$lpersonalheight\t$lrating\t$llastgone\t$lvisitno\t$laddjy\t$lmeili\t$lmymoney\t$lpostdel\t$lsex\t$leducation\t$lmarry\t$lwork\t$lborn\t$luseradd1\t$luseradd2\t";
	close(FILE7);
	}
	&winunlock($filetomake) if ($OS_USED eq "Nt");
   }
}
